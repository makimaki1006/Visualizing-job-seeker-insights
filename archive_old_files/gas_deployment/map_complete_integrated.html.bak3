    // クロス集計実行
    function executeCrossTabulation(city, applicants, axisX, axisY) {
      const resultDiv = document.getElementById('crossResult');

      if (!applicants || applicants.length === 0) {
        resultDiv.innerHTML = `<div class="stats" style="color:#999;">データがありません。</div>`;
        return;
      }

      // 軸ラベル取得
      const labelX = getAxisLabel(axisX);
      const labelY = getAxisLabel(axisY);

      // データ処理
      const processedData = applicants.map(app => ({
        x: extractAxisValue(app, axisX),
        y: extractAxisValue(app, axisY)
      }));

      // クロス集計
      const crossMatrix = buildCrossMatrix(processedData);

      if (!crossMatrix || crossMatrix.rows.length === 0 || crossMatrix.cols.length === 0) {
        resultDiv.innerHTML = `<div class="stats" style="color:#999;">集計可能なデータがありません。軸の組み合わせを変更してください。</div>`;
        return;
      }

      // HTML生成
      const html = `
        <div class="section">
          <h3>${labelY} × ${labelX}</h3>
          <div style="display:flex; gap:10px; margin-bottom:10px;">
            <button onclick="downloadCrossCSV('${labelY}', '${labelX}')" style="padding:5px 10px; background:#555; color:white; border:none; border-radius:4px; cursor:pointer; font-size:12px;">
              📥 CSV出力
            </button>
          </div>

          <div style="overflow-x:auto; margin-bottom:20px;">
            ${buildCrossTable(crossMatrix, labelY, labelX)}
          </div>

          <div id="crossHeatmapChart" style="width:100%; height:400px;"></div>
        </div>
      `;

      resultDiv.innerHTML = html;

      // ヒートマップチャート描画
      renderCrossHeatmap(crossMatrix, labelY, labelX);

      // CSVダウンロード用にグローバル変数に保存
      window.currentCrossMatrix = crossMatrix;
      window.currentCrossLabels = { labelY, labelX };
    }

    // 軸ラベル取得
    function getAxisLabel(axis) {
      if (axis.startsWith('qual:')) {
        return axis.replace('qual:', '');
      }
      const labels = {
        age_bucket: '年齢層',
        gender: '性別',
        employment_status: '就業状態',
        education: '学歴',
        career: 'キャリア',
        desired_job: '希望職種',
        urgency_level: '転職意欲'
      };
      return labels[axis] || axis;
    }

    // 軸の値を抽出
    function extractAxisValue(applicant, axis) {
      // 資格バイネーム
      if (axis.startsWith('qual:')) {
        const qualName = axis.replace('qual:', '');
        const qualifications = (applicant.qualifications || '').split(',').map(q => q.trim());
        return qualifications.includes(qualName) ? 'あり' : 'なし';
      }

      // 基本属性
      if (axis === 'age_bucket') return applicant.age_bucket || '不明';
      if (axis === 'gender') return applicant.gender || '不明';
      if (axis === 'employment_status') return applicant.employment_status || '不明';
      if (axis === 'education') return applicant.education || '不明';
      if (axis === 'career') return applicant.career || '不明';
      if (axis === 'desired_job') return applicant.desired_job || '不明';
      if (axis === 'urgency_level') {
        const score = applicant.urgency_score;
        if (score === undefined || score === null) return '不明';
        if (score >= 8) return '最高';
        if (score >= 6) return '高';
        if (score >= 4) return '中';
        return '低';
      }

      return '不明';
    }

    // クロス集計マトリックス構築
    function buildCrossMatrix(data) {
      const matrix = {};
      const rowSet = new Set();
      const colSet = new Set();

      // データをマトリックスに格納
      data.forEach(item => {
        const x = item.x;
        const y = item.y;
        if (!matrix[y]) matrix[y] = {};
        if (!matrix[y][x]) matrix[y][x] = 0;
        matrix[y][x]++;
        rowSet.add(y);
        colSet.add(x);
      });

      const rows = Array.from(rowSet).sort();
      const cols = Array.from(colSet).sort();

      return { matrix, rows, cols };
    }

    // クロス集計テーブル生成
    function buildCrossTable(crossMatrix, labelY, labelX) {
      const { matrix, rows, cols } = crossMatrix;

      let html = `<table style="width:100%; border-collapse:collapse; font-size:13px;">`;

      // ヘッダー
      html += `<thead><tr style="background:#f0f0f0;"><th style="border:1px solid #ddd; padding:8px; text-align:left;">${labelY} \\ ${labelX}</th>`;
      cols.forEach(col => {
        html += `<th style="border:1px solid #ddd; padding:8px; text-align:center;">${col}</th>`;
      });
      html += `<th style="border:1px solid #ddd; padding:8px; text-align:center; background:#e8e8e8;">合計</th></tr></thead>`;

      // ボディ
      html += `<tbody>`;
      rows.forEach(row => {
        html += `<tr>`;
        html += `<td style="border:1px solid #ddd; padding:8px; font-weight:bold;">${row}</td>`;
        let rowTotal = 0;
        cols.forEach(col => {
          const count = matrix[row][col] || 0;
          rowTotal += count;
          const bgColor = count > 0 ? `rgba(66, 133, 244, ${Math.min(count / 50, 0.8)})` : '#fff';
          html += `<td style="border:1px solid #ddd; padding:8px; text-align:center; background:${bgColor};">${count}</td>`;
        });
        html += `<td style="border:1px solid #ddd; padding:8px; text-align:center; background:#f8f8f8; font-weight:bold;">${rowTotal}</td>`;
        html += `</tr>`;
      });

      // 合計行
      html += `<tr style="background:#e8e8e8; font-weight:bold;">`;
      html += `<td style="border:1px solid #ddd; padding:8px;">合計</td>`;
      let grandTotal = 0;
      cols.forEach(col => {
        let colTotal = 0;
        rows.forEach(row => {
          colTotal += matrix[row][col] || 0;
        });
        grandTotal += colTotal;
        html += `<td style="border:1px solid #ddd; padding:8px; text-align:center;">${colTotal}</td>`;
      });
      html += `<td style="border:1px solid #ddd; padding:8px; text-align:center; background:#ddd;">${grandTotal}</td>`;
      html += `</tr>`;

      html += `</tbody></table>`;

      return html;
    }

    // ヒートマップチャート描画
    function renderCrossHeatmap(crossMatrix, labelY, labelX) {
      const { matrix, rows, cols } = crossMatrix;

      // Google Charts用データ準備
      const chartData = [
        [labelY, ...cols]
      ];

      rows.forEach(row => {
        const rowData = [row];
        cols.forEach(col => {
          rowData.push(matrix[row][col] || 0);
        });
        chartData.push(rowData);
      });

      const data = google.visualization.arrayToDataTable(chartData);

      const options = {
        title: `${labelY} × ${labelX} ヒートマップ`,
        hAxis: { title: labelX },
        vAxis: { title: labelY },
        legend: { position: 'right' },
        colors: ['#e0f2f1', '#80cbc4', '#4db6ac', '#26a69a', '#00897b'],
        isStacked: true
      };

      const chart = new google.visualization.ColumnChart(document.getElementById('crossHeatmapChart'));
      chart.draw(data, options);
    }

    // CSV出力
    function downloadCrossCSV(labelY, labelX) {
      const crossMatrix = window.currentCrossMatrix;
      if (!crossMatrix) return;

      const { matrix, rows, cols } = crossMatrix;

      let csv = `${labelY}\\${labelX},${cols.join(',')},合計\n`;

      rows.forEach(row => {
        let rowData = [row];
        let rowTotal = 0;
        cols.forEach(col => {
          const count = matrix[row][col] || 0;
          rowData.push(count);
          rowTotal += count;
        });
        rowData.push(rowTotal);
        csv += rowData.join(',') + '\n';
      });

      // 合計行
      let totals = ['合計'];
      let grandTotal = 0;
      cols.forEach(col => {
        let colTotal = 0;
        rows.forEach(row => {
          colTotal += matrix[row][col] || 0;
        });
        totals.push(colTotal);
        grandTotal += colTotal;
      });
      totals.push(grandTotal);
      csv += totals.join(',') + '\n';

      // BOM付きUTF-8でダウンロード
      const bom = '\uFEFF';
      const blob = new Blob([bom + csv], { type: 'text/csv;charset=utf-8;' });
      const link = document.createElement('a');
      link.href = URL.createObjectURL(blob);
      link.download = `cross_${labelY}_${labelX}_${new Date().toISOString().slice(0,10)}.csv`;
      link.click();
    }

    function renderFlowArrows(city) {
      // 地図が初期化されていない場合は終了
      if (!map) {
        console.warn('renderFlowArrows: 地図が初期化されていません');
        return;
      }

      // 既存のフローレイヤーをクリア
      if (flowLayer) {
        flowLayer.clearLayers();
      } else {
        flowLayer = L.layerGroup().addTo(map);
      }

      const flow = city.flow || {};
      const topInflows = flow.top_inflows || [];
      const topOutflows = flow.top_outflows || [];

      // Phase1_MapMetricsから座標データを取得（全地域）
      const mapMetricsData = DATA.flatMap(c => (c.overview && c.overview.map_metrics) || []);

      // 座標マップを作成（地域キー → {lat, lng}）
      const coordMap = {};
      mapMetricsData.forEach(record => {
        const pref = String(record.prefecture || '').trim();
        const muni = String(record.municipality || '').trim();
        const lat = Number(record.latitude || record.lat);
        const lng = Number(record.longitude || record.lng || record.lon);

        if (pref && muni && Number.isFinite(lat) && Number.isFinite(lng)) {
          const key = `${pref}${muni}`;
          coordMap[key] = { lat, lng };
        }
      });

      // 現在の地域の座標
      const currentCenter = city.center;
      if (!currentCenter || !Array.isArray(currentCenter) || currentCenter.length < 2) {
        return; // 座標がない場合は矢印を描画しない
      }

      const currentLat = currentCenter[0];
      const currentLng = currentCenter[1];

      // 流入矢印を描画（origin → current）
      topInflows.forEach((flow, index) => {
        const originPref = String(flow.origin_pref || '').trim();
        const originMuni = String(flow.origin_muni || '').trim();
        const originKey = `${originPref}${originMuni}`;
        const originCoord = coordMap[originKey];

        if (!originCoord) {
          return; // 座標が見つからない場合はスキップ
        }

        const flowCount = Number(flow.flow_count || 0);
        const weight = Math.max(2, Math.min(10, flowCount / 10)); // 2-10pxの範囲で太さを調整
        const opacity = Math.min(0.8, 0.3 + (flowCount / 100)); // 流入数に応じて透明度調整

        // ポリライン（曲線）を描画
        const latlngs = [
          [originCoord.lat, originCoord.lng],
          [currentLat, currentLng]
        ];

        const polyline = L.polyline(latlngs, {
          color: '#38bdf8', // 青色（流入）
          weight: weight,
          opacity: opacity,
          dashArray: '5, 5' // 破線
        }).addTo(flowLayer);

        // 矢印装飾を追加
        const decorator = L.polylineDecorator(polyline, {
          patterns: [
            {
              offset: '50%',
              repeat: 0,
              symbol: L.Symbol.arrowHead({
                pixelSize: 10,
                polygon: false,
                pathOptions: {
                  color: '#38bdf8',
                  fillOpacity: 1,
                  weight: 2
                }
              })
            }
          ]
        }).addTo(flowLayer);

        // ポップアップを追加
        polyline.bindPopup(`
          <strong>流入: ${flow.origin || '-'}</strong><br>
          人数: ${numberFmt.format(flowCount)}人<br>
          平均年齢: ${flow.avg_age ? Number(flow.avg_age).toFixed(1) : '-'}歳<br>
          性別: ${flow.gender_mode || '-'}
        `);
      });

      // 流出矢印を描画（current → destination）
      topOutflows.forEach((flow, index) => {
        const destPref = String(flow.destination_pref || '').trim();
        const destMuni = String(flow.destination_muni || '').trim();
        const destKey = `${destPref}${destMuni}`;
        const destCoord = coordMap[destKey];

        if (!destCoord) {
          return; // 座標が見つからない場合はスキップ
        }

        const flowCount = Number(flow.flow_count || 0);
        const weight = Math.max(2, Math.min(10, flowCount / 10)); // 2-10pxの範囲で太さを調整
        const opacity = Math.min(0.8, 0.3 + (flowCount / 100)); // 流出数に応じて透明度調整

        // ポリライン（曲線）を描画
        const latlngs = [
          [currentLat, currentLng],
          [destCoord.lat, destCoord.lng]
        ];

        const polyline = L.polyline(latlngs, {
          color: '#f97316', // オレンジ色（流出）
          weight: weight,
          opacity: opacity,
          dashArray: '5, 5' // 破線
        }).addTo(flowLayer);

        // 矢印装飾を追加
        const decorator = L.polylineDecorator(polyline, {
          patterns: [
            {
              offset: '50%',
              repeat: 0,
              symbol: L.Symbol.arrowHead({
                pixelSize: 10,
                polygon: false,
                pathOptions: {
                  color: '#f97316',
                  fillOpacity: 1,
                  weight: 2
                }
              })
            }
          ]
        }).addTo(flowLayer);

        // ポップアップを追加
        polyline.bindPopup(`
          <strong>流出: ${flow.destination || '-'}</strong><br>
          人数: ${numberFmt.format(flowCount)}人<br>
          平均年齢: ${flow.avg_age ? Number(flow.avg_age).toFixed(1) : '-'}歳<br>
          性別: ${flow.gender_mode || '-'}
        `);
      });
    }

    function renderFlow(city){
      const panel = qs('.panel[data-panel="flow"]');
      const flow = city.flow || {};

      // nearby_regions形式のデータを変換
      const nearbyRegions = flow.nearby_regions || [];
      const inflows = nearbyRegions.filter(r => r.type === 'inflow');
      const outflows = nearbyRegions.filter(r => r.type === 'outflow');

      // サマリー計算
      const totalInflow = inflows.reduce((sum, r) => sum + (r.flow_count || 0), 0);
      const totalOutflow = outflows.reduce((sum, r) => sum + (r.flow_count || 0), 0);
      const netFlow = totalInflow - totalOutflow;

      const blocks = [];
      const highlights = [];

      // 重要指標（KPI）
      if (totalInflow > 0) {
        highlights.push({ label: '総流入数', main: numberFmt.format(totalInflow), sub: '人' });
      }
      if (totalOutflow > 0) {
        highlights.push({ label: '総流出数', main: numberFmt.format(totalOutflow), sub: '人' });
      }
      if (totalInflow > 0 || totalOutflow > 0) {
        const netLabel = netFlow > 0 ? '純流入' : (netFlow < 0 ? '純流出' : '流入流出均衡');
        highlights.push({
          label: '純フロー',
          main: (netFlow > 0 ? '+' : '') + numberFmt.format(netFlow),
          sub: netLabel
        });
      }
      if (inflows.length > 0) {
        highlights.push({ label: '流入元地域数', main: numberFmt.format(inflows.length), sub: '地域' });
      }
      if (outflows.length > 0) {
        highlights.push({ label: '流出先地域数', main: numberFmt.format(outflows.length), sub: '地域' });
      }

      if (highlights.length) {
        const chipHtml = highlights.map(item => `<div class="chip"><span class="chip-label">${item.label}</span><strong>${item.main}</strong>${item.sub ? `<span class="chip-sub">${item.sub}</span>` : ''}</div>`).join('');
        blocks.push(`<div class="section flow-highlights"><h2>フロー分析 重点指標</h2><div class="chip-grid">${chipHtml}</div></div>`);
      }

      // 流入テーブル
      if (inflows.length > 0) {
        const inflowRows = inflows
          .sort((a, b) => (b.flow_count || 0) - (a.flow_count || 0))
          .map(r => `
            <tr>
              <td>${r.prefecture || ''} ${r.municipality || ''}</td>
              <td style="text-align:right">${numberFmt.format(r.flow_count || 0)}人</td>
            </tr>
          `).join('');

        blocks.push(`
          <div class="section">
            <h2>流入元地域（全${inflows.length}地域）</h2>
            <div class="table-wrap">
              <table class="data-table">
                <thead>
                  <tr>
                    <th>流入元</th>
                    <th style="text-align:right">流入数</th>
                  </tr>
                </thead>
                <tbody>${inflowRows}</tbody>
              </table>
            </div>
          </div>
        `);
      }

      // 流出テーブル
      if (outflows.length > 0) {
        const outflowRows = outflows
          .sort((a, b) => (b.flow_count || 0) - (a.flow_count || 0))
          .map(r => `
            <tr>
              <td>${r.prefecture || ''} ${r.municipality || ''}</td>
              <td style="text-align:right">${numberFmt.format(r.flow_count || 0)}人</td>
            </tr>
          `).join('');

        blocks.push(`
          <div class="section">
            <h2>流出先地域（全${outflows.length}地域）</h2>
            <div class="table-wrap">
              <table class="data-table">
                <thead>
                  <tr>
                    <th>流出先</th>
                    <th style="text-align:right">流出数</th>
                  </tr>
                </thead>
                <tbody>${outflowRows}</tbody>
              </table>
            </div>
          </div>
        `);
      }

      if (!blocks.length) {
        panel.innerHTML = `<div class="section"><div class="stats">フロー分析データが存在しません。</div></div>`;
        return;
      }

      panel.innerHTML = blocks.join('');

      // 地図上に矢印フローを描画
      renderFlowArrows(city);
    }

    // -------- Phase 12: 需給バランス（選択地域軸） --------
    function renderGap(city){
      const panel = qs('.panel[data-panel="gap"]');
      const g = city.gap || {};

      // データ存在チェック（新データ構造対応）
      const hasTopGaps = g.top_gaps && g.top_gaps.length > 0;
      const hasSummary = g.summary && (g.summary.total_demand !== undefined || g.summary.total_supply !== undefined);

      if (!hasTopGaps && !hasSummary) {
        panel.innerHTML = `<div class="section"><div class="stats">この地域の需給データがありません。</div></div>`;
        return;
      }

      // データ取得（top_gapsの最初の要素、またはsummaryから）
      const gapData = hasTopGaps ? g.top_gaps[0] : {};
      const demand = gapData.demand_count || (g.summary && g.summary.total_demand) || 0;
      const supply = gapData.supply_count || (g.summary && g.summary.total_supply) || 0;
      const ratio = gapData.demand_supply_ratio || (g.summary && g.summary.avg_ratio) || 0;
      const gap = gapData.gap || (demand - supply) || 0;

      // 需給バランス評価
      let balanceStatus = '均衡';
      let balanceColor = COLOR[3]; // 緑
      let balanceIcon = '⚖️';

      if (gap > supply * 0.5) {
        balanceStatus = '深刻な人材不足';
        balanceColor = COLOR[1]; // オレンジ
        balanceIcon = '🚨';
      } else if (gap > 0) {
        balanceStatus = '人材不足';
        balanceColor = COLOR[4]; // 黄色
        balanceIcon = '⚠️';
      } else if (gap < -supply * 0.3) {
        balanceStatus = '人材過剰';
        balanceColor = COLOR[0]; // 青
        balanceIcon = '📊';
      }

      // KPI表示
      const kpis = `
        <div class="kpis">
          <div class="kpi"><div class="label">需要（希望者数）</div><div class="value">${numberFmt.format(demand)}人</div></div>
          <div class="kpi"><div class="label">供給（居住者数）</div><div class="value">${numberFmt.format(supply)}人</div></div>
          <div class="kpi"><div class="label">需給比率</div><div class="value">${Number(ratio).toFixed(2)}</div></div>
          <div class="kpi" style="background:${balanceColor}33;border-color:${balanceColor}"><div class="label">需給バランス</div><div class="value">${balanceIcon} ${balanceStatus}</div></div>
        </div>
      `;

      // サマリー
      const summaryHtml = `
        <div class="section">
          <h2>需給バランス詳細</h2>
          <div class="stats">
            <div class="row"><span>選択地域</span><span>${city.name || g.location || '-'}</span></div>
            <div class="row"><span>この地域を希望する人材数</span><span>${numberFmt.format(demand)}人</span></div>
            <div class="row"><span>この地域に居住する人材数</span><span>${numberFmt.format(supply)}人</span></div>
            <div class="row"><span>需給ギャップ</span><span>${gap > 0 ? '+' : ''}${numberFmt.format(gap)}人 ${gap > 0 ? '（不足）' : gap < 0 ? '（過剰）' : '（均衡）'}</span></div>
            <div class="row"><span>需給比率</span><span>${Number(ratio).toFixed(2)} （需要÷供給+1）</span></div>
            <div class="row"><span>評価</span><span style="color:${balanceColor}">${balanceIcon} ${balanceStatus}</span></div>
          </div>
        </div>
      `;

      // 解釈ガイド
      const interpretationHtml = `
        <div class="section">
          <h2>データの見方</h2>
          <div class="stats">
            <div class="row"><span>需要（希望者数）</span><span>この地域で働きたい人の数</span></div>
            <div class="row"><span>供給（居住者数）</span><span>この地域に住んでいる人の数</span></div>
            <div class="row"><span>需給ギャップ</span><span>需要 - 供給（プラスなら人材不足、マイナスなら人材過剰）</span></div>
            <div class="row"><span>需給比率</span><span>需要の大きさを示す指標（高いほど人気・競争激しい）</span></div>
          </div>
        </div>
      `;

      panel.innerHTML = `
        ${kpis}
        ${summaryHtml}
        ${interpretationHtml}
        <div class="section">
          <h2>需給可視化</h2>
          <div class="chart-grid">
            <div class="chart-card"><header>需要 vs 供給</header><div class="chart-body"><canvas id="gapCompareChart"></canvas></div></div>
            <div class="chart-card"><header>需給バランス</header><div class="chart-body"><canvas id="gapBalanceChart"></canvas></div></div>
          </div>
        </div>
      `;

      // 需要 vs 供給チャート（Bar）
      upsertChart('gapCompareChart', {
        type:'bar',
        data:{
          labels:['需要と供給'],
          datasets:[
            {label:'需要（希望者数）', data:[demand], backgroundColor:COLOR[0]},
            {label:'供給（居住者数）', data:[supply], backgroundColor:COLOR[1]}
          ]
        },
        options:{
          ...chartBase(),
          scales:{y:{beginAtZero:true}},
          plugins:{
            ...chartBase().plugins,
            legend:{display:true, position:'bottom', labels:{color:getComputedStyle(document.documentElement).getPropertyValue('--text')}}
          }
        }
      });

      // 需給バランスチャート（Doughnut）
      const gapAbs = Math.abs(gap);
      const balanceData = gap > 0
        ? [gap, supply]
        : gap < 0
          ? [demand, gapAbs]
          : [demand, supply];
      const balanceLabels = gap > 0
        ? ['不足分', '供給']
        : gap < 0
          ? ['需要', '過剰分']
          : ['需要', '供給'];

      upsertChart('gapBalanceChart', {
        type:'doughnut',
        data:{
          labels:balanceLabels,
          datasets:[{
            data:balanceData,
            backgroundColor: gap > 0 ? [COLOR[1], COLOR[3]] : gap < 0 ? [COLOR[0], COLOR[4]] : [COLOR[0], COLOR[1]]
          }]
        },
        options:{
          ...chartBase(),
          plugins:{
            ...chartBase().plugins,
            legend:{display:true, position:'bottom', labels:{color:getComputedStyle(document.documentElement).getPropertyValue('--text')}}
          }
        }
      });
    }

    // -------- Phase 13: 希少人材分析（選択地域軸） --------
    function renderRarity(city){
      const panel = qs('.panel[data-panel="rarity"]');
      const r = city.rarity || {};

      // 選択地域のデータ（新データ構造対応）
      const locationRecords = r.top_rarity || [];
      const summary = r.summary || {};

      // データ存在チェック
      if (locationRecords.length === 0) {
        panel.innerHTML = `<div class="section"><div class="stats">この地域を希望する人材の希少性データがありません。</div></div>`;
        return;
      }

      // 希少性ランク分布を計算
      const rankDistribution = r.rank_distribution || {};

      // ランクごとの集計（summaryから取得、またはlocationRecordsから計算）
      const sRank = summary.s_rank || locationRecords.filter(rec => rec.rarity_rank && rec.rarity_rank.startsWith('S')).length;
      const aRank = summary.a_rank || locationRecords.filter(rec => rec.rarity_rank && rec.rarity_rank.startsWith('A')).length;
      const bRank = summary.b_rank || locationRecords.filter(rec => rec.rarity_rank && rec.rarity_rank.startsWith('B')).length;
      const cRank = locationRecords.filter(rec => rec.rarity_rank && rec.rarity_rank.startsWith('C')).length;

      // Top 10希少性スコア（すでにcreateRarityData()でソート済み）
      const topRarity = locationRecords.slice(0, 10);

      // KPI表示
      const kpis = `
        <div class="kpis">
          <div class="kpi"><div class="label">属性組み合わせ数</div><div class="value">${numberFmt.format(locationRecords.length)}</div></div>
          <div class="kpi"><div class="label">超希少（S）</div><div class="value">${numberFmt.format(sRank)}件</div></div>
          <div class="kpi"><div class="label">非常に希少（A）</div><div class="value">${numberFmt.format(aRank)}件</div></div>
          <div class="kpi"><div class="label">希少（B）</div><div class="value">${numberFmt.format(bRank)}件</div></div>
        </div>
      `;

      // ランク分布サマリー
      const rankRows = Object.entries(rankDistribution)
        .sort((a, b) => b[1] - a[1])
        .map(([k, v]) => `<div class="row"><span>${k}</span><span>${numberFmt.format(v)}件</span></div>`)
        .join('');

      // Top 10希少人材テーブル
      const rarityRows = topRarity.map(item => `
        <tr>
          <td>${item.age_bucket || '-'}</td>
          <td>${item.gender || '-'}</td>
          <td>${item.has_national_license ? 'あり' : 'なし'}</td>
          <td>${numberFmt.format(item.count || 0)}人</td>
          <td>${Number(item.rarity_score || 0).toFixed(2)}</td>
          <td><span style="color:${item.rarity_rank && item.rarity_rank.startsWith('S') ? COLOR[1] : item.rarity_rank && item.rarity_rank.startsWith('A') ? COLOR[4] : COLOR[3]}">${item.rarity_rank || '-'}</span></td>
        </tr>
      `).join('');

      // 解釈ガイド
      const interpretationHtml = `
        <div class="section">
          <h2>希少性ランクの見方</h2>
          <div class="stats">
            <div class="row"><span>S: 超希少</span><span>1人のみ（希少性スコア1.0）</span></div>
            <div class="row"><span>A: 非常に希少</span><span>2人のみ（希少性スコア0.5）</span></div>
            <div class="row"><span>B: 希少</span><span>3-5人（希少性スコア0.2-0.33）</span></div>
            <div class="row"><span>C: やや希少</span><span>6-20人（希少性スコア0.05-0.17）</span></div>
            <div class="row"><span>D: 一般的</span><span>20人超（希少性スコア<0.05）</span></div>
          </div>
        </div>
      `;

      panel.innerHTML = `
        ${kpis}
        <div class="section">
          <h2>希少性ランク分布</h2>
          <div class="stats">${rankRows}</div>
        </div>
        <div class="section">
          <h2>最も希少な人材Top 10</h2>
          <div class="table-wrap">
            <table class="data-table">
              <thead><tr><th>年齢層</th><th>性別</th><th>国家資格</th><th>人数</th><th>希少性スコア</th><th>ランク</th></tr></thead>
              <tbody>${rarityRows}</tbody>
            </table>
          </div>
        </div>
        ${interpretationHtml}
        <div class="section">
          <h2>希少性可視化</h2>
          <div class="chart-grid">
            <div class="chart-card"><header>希少性ランク分布</header><div class="chart-body"><canvas id="rarityRankChart"></canvas></div></div>
            <div class="chart-card"><header>Top 10 希少性スコア</header><div class="chart-body"><canvas id="rarityScoreChart"></canvas></div></div>
          </div>
        </div>
      `;

      // ランク分布チャート（Doughnut）
      const rankEntries = Object.entries(rankDistribution).sort((a, b) => b[1] - a[1]);
      if (rankEntries.length) {
        upsertChart('rarityRankChart', {
          type: 'doughnut',
          data: {
            labels: rankEntries.map(([k]) => k),
            datasets: [{
              data: rankEntries.map(([, v]) => v),
              backgroundColor: COLOR.slice(0, rankEntries.length)
            }]
          },
          options: {
            ...chartBase(),
            plugins: {
              ...chartBase().plugins,
              legend: { display: true, position: 'bottom', labels: { color: getComputedStyle(document.documentElement).getPropertyValue('--text') } }
            }
          }
        });
      }

      // スコアチャート（Bar）
      if (topRarity.length) {
        upsertChart('rarityScoreChart', {
          type: 'bar',
          data: {
            labels: topRarity.map(item => `${item.age_bucket || '-'} ${item.gender || '-'} ${item.has_national_license ? '有資格' : ''}`),
            datasets: [{
              label: '希少性スコア',
              data: topRarity.map(item => item.rarity_score || 0),
              backgroundColor: topRarity.map(item =>
                item.rarity_rank && item.rarity_rank.startsWith('S') ? COLOR[1] :
                item.rarity_rank && item.rarity_rank.startsWith('A') ? COLOR[4] :
                COLOR[3]
              )
            }]
          },
          options: { ...chartBase(), indexAxis: 'y', scales: { x: { beginAtZero: true, max: 1 } } }
        });
      }
    }

    // -------- Phase 14: 人材プロファイル（選択地域軸） --------
    function renderCompetition(city){
      const panel = qs('.panel[data-panel="competition"]');
      const c = city.competition || {};
      const summary = c.summary || {};
      const topLocations = c.top_locations || [];

      // データ存在チェック（新データ構造対応）
      if (!summary.total_applicants || summary.total_applicants === 0) {
        panel.innerHTML = `<div class="section"><div class="stats">この地域を希望する人材データがありません。</div></div>`;
        return;
      }

      // top_locationsの最初の要素からデータ取得
      const locationData = topLocations.length > 0 ? topLocations[0] : {};

      // KPI表示
      const kpis = `
        <div class="kpis">
          <div class="kpi"><div class="label">総希望者数</div><div class="value">${numberFmt.format(summary.total_applicants||0)}人</div></div>
          <div class="kpi"><div class="label">最多年齢層</div><div class="value">${locationData.top_age_group||'-'}</div></div>
          <div class="kpi"><div class="label">女性比率</div><div class="value">${percentFmt.format(summary.avg_female_ratio||locationData.female_ratio||0)}</div></div>
          <div class="kpi"><div class="label">国家資格保有率</div><div class="value">${percentFmt.format(summary.avg_national_license_rate||locationData.national_license_rate||0)}</div></div>
        </div>
      `;

      // プロファイルサマリー
      const summaryHtml = `
        <div class="section">
          <h2>人材プロファイルサマリー</h2>
          <div class="stats">
            <div class="row"><span>選択地域</span><span>${city.name || locationData.location || '-'}</span></div>
            <div class="row"><span>この地域を希望する人材数</span><span>${numberFmt.format(summary.total_applicants||0)}人</span></div>
            <div class="row"><span>最も多い年齢層</span><span>${locationData.top_age_group||'-'} (${percentFmt.format(locationData.top_age_ratio||0)})</span></div>
            <div class="row"><span>性別比率</span><span>女性 ${percentFmt.format(locationData.female_ratio||summary.avg_female_ratio||0)} / 男性 ${percentFmt.format(locationData.male_ratio||(1-(summary.avg_female_ratio||0))||0)}</span></div>
            <div class="row"><span>国家資格保有率</span><span>${percentFmt.format(locationData.national_license_rate||summary.avg_national_license_rate||0)}</span></div>
            <div class="row"><span>最も多い就業状態</span><span>${locationData.top_employment_status||'-'} (${percentFmt.format(locationData.top_employment_ratio||0)})</span></div>
            <div class="row"><span>平均保有資格数</span><span>${Number(locationData.avg_qualification_count||0).toFixed(2)}個</span></div>
          </div>
        </div>
      `;

      panel.innerHTML = `
        ${kpis}
        ${summaryHtml}
        <div class="section">
          <h2>属性分布</h2>
          <div class="chart-grid">
            <div class="chart-card"><header>性別分布</header><div class="chart-body"><canvas id="compGenderChart"></canvas></div></div>
            <div class="chart-card"><header>年齢層・就業状態</header><div class="chart-body"><canvas id="compAgeEmploymentChart"></canvas></div></div>
          </div>
        </div>
      `;

      // 性別分布チャート（Pie）
      upsertChart('compGenderChart', {
        type:'doughnut',
        data:{
          labels:['女性', '男性'],
          datasets:[{
            data:[
              (locationData.female_ratio||summary.avg_female_ratio||0) * (summary.total_applicants||0),
              (locationData.male_ratio||(1-(summary.avg_female_ratio||0))||0) * (summary.total_applicants||0)
            ],
            backgroundColor:[COLOR[5], COLOR[0]]
          }]
        },
        options:{
          ...chartBase(),
          plugins:{
            ...chartBase().plugins,
            legend:{display:true, position:'bottom', labels:{color:getComputedStyle(document.documentElement).getPropertyValue('--text')}}
          }
        }
      });

      // 年齢層・就業状態チャート（Bar）
      upsertChart('compAgeEmploymentChart', {
        type:'bar',
        data:{
          labels:['年齢層', '就業状態'],
          datasets:[
            {label:locationData.top_age_group||'-', data:[locationData.top_age_ratio||0, 0], backgroundColor:COLOR[2]},
            {label:locationData.top_employment_status||'-', data:[0, locationData.top_employment_ratio||0], backgroundColor:COLOR[3]}
          ]
        },
        options:{
          ...chartBase(),
          scales:{
            y:{beginAtZero:true, max:1, ticks:{callback:v=>percentFmt.format(v)}}
          }
        }
      });
    }

    function renderCity(){
      const c = DATA[activeCity];
      if(!c){
        qs('#cityName').textContent = '-';
        qs('#cityMeta').textContent = 'データを取得できませんでした';
        qsa('.panel').forEach(p=> p.innerHTML = `<div class="section"><div class="stats">データなし</div></div>`);
        return;
      }
      qs('#cityName').textContent = c.name || '-';
      const ppl = (c.overview && c.overview.kpis)?.find(k=>k.label==='求職者数');
      const upd = c.updated ? ` / 更新日: ${c.updated}` : '';
      qs('#cityMeta').textContent = `求職者数: ${ppl?numberFmt.format(ppl.value):'-'}人${upd}`;
      if(c.region){
        selectedRegion = c.region;
      }
      const badge = qs('#qualityBadge');
      if(badge){
        const q = c.quality || {};
        if(q && (q.status || (Array.isArray(q.issues)&&q.issues.length) || (Array.isArray(q.warnings)&&q.warnings.length))){
          const baseColor = q.color || '#38bdf8';
          badge.textContent = q.label || '品質情報';
          const tooltipParts = [];
          if(q.message){ tooltipParts.push(q.message); }
          if(Array.isArray(q.warnings) && q.warnings.length){ tooltipParts.push(q.warnings.join('\n')); }
          badge.title = tooltipParts.filter(Boolean).join('\n');
          badge.style.background = toRGBA(baseColor, 0.22);
          badge.style.borderColor = toRGBA(baseColor, 0.55);
          badge.style.color = '#f8fafc';
          badge.dataset.status = q.status || 'notice';
          badge.hidden = false;
        } else {
          badge.hidden = true;
          badge.title = '';
        }
      }
      const prefSel = qs('#prefectureSelect');
      const muniSel = qs('#municipalitySelect');
      if(prefSel && muniSel){
        if(availableRegions.length && c.region){
          // 都道府県を更新
          if(c.region.prefecture){
            if(prefSel.value !== c.region.prefecture){
              prefSel.value = c.region.prefecture;
              selectedPrefecture = c.region.prefecture;
            }
            // 都道府県が変わっていなくても、市区町村プルダウンが正しく設定されているか確認
            if(selectedPrefecture !== c.region.prefecture || muniSel.value !== c.region.key){
              selectedPrefecture = c.region.prefecture;
              populateMunicipalitySelector(c.region.prefecture);
            }
          }
          // 市区町村を更新
          if(c.region.key && muniSel.value !== c.region.key){
            muniSel.value = c.region.key;
          }
        }
      }

      renderOverview(c);
      renderSupply(c);
      renderCareer(c);
      renderUrgency(c);
      renderPersona(c);
      renderCross(c);
      renderFlow(c);
      renderGap(c);
      renderRarity(c);
      renderCompetition(c);
      renderDashboard(c);
      syncTabs();
      panToCity();
      renderMarkers();
    }

    function initResizeObserver(){
      if(ro) ro.disconnect();
      ro = new ResizeObserver(()=> requestAnimationFrame(()=> Object.values(charts).forEach(ch=> ch.resize())) );
      ro.observe(sidebar);
    }

    function renderAll(){
      renderTabbar();
      populateSelector();
      if(!map){ initMap(); }
      initResizeObserver();
      renderCity();
    }

    function applyPayload(payload){
      showProgress(75, 'データを適用中...');

      console.log('[applyPayload] ペイロード受信:', payload);

      const normalized = normalizePayload(payload);

      console.log('[applyPayload] 正規化後:', normalized);
      console.log('[applyPayload] - cities: ' + normalized.cities.length + '件');
      console.log('[applyPayload] - regionOptions:', normalized.regionOptions);
      console.log('[applyPayload] - availableRegions: ' + normalized.availableRegions.length + '件');

      const effectiveCities = normalized.cities.length
        ? normalized.cities
        : (DATA.length ? DATA : fallbackPayload.cities);
      DATA = effectiveCities.slice();
      const resolvedRegion = normalized.selectedRegion || selectedRegion || fallbackPayload.selectedRegion;
      selectedRegion = resolvedRegion;
      regionOptions = normalized.regionOptions || regionOptions || fallbackPayload.regionOptions;
      if(normalized.availableRegions.length){
        availableRegions = normalized.availableRegions.slice();
      } else if(!availableRegions.length){
        availableRegions = fallbackPayload.availableRegions.slice();
      }

      console.log('[applyPayload] 適用後:');
      console.log('  - regionOptions.prefectures: ' + (regionOptions?.prefectures?.length || 0) + '件');
      console.log('  - availableRegions: ' + availableRegions.length + '件');
      let defaultIndex = 0;
      if(selectedRegion && selectedRegion.key){
        const found = DATA.findIndex(city => (city.region && city.region.key === selectedRegion.key) || city.id === selectedRegion.key);
        if(found >= 0){
          defaultIndex = found;
        }
      }
      activeCity = defaultIndex;

      showProgress(90, '画面を描画中...');
      setTimeout(()=> {
        renderAll();
        showProgress(100, '完了');
        setTimeout(hideProgress, 300);
      }, 100);
    }

    // 進捗バー制御
    function showProgress(percent, message){
      const progressDiv = document.getElementById('loadingProgress');
      const progressBar = document.getElementById('progressBar');
      const progressText = document.getElementById('progressText');
      if(progressDiv && progressBar && progressText){
        progressDiv.style.display = 'block';
        progressBar.style.width = percent + '%';
        progressText.textContent = message;
      }
    }

    function hideProgress(){
      const progressDiv = document.getElementById('loadingProgress');
      if(progressDiv){
        setTimeout(()=> progressDiv.style.display = 'none', 300);
      }
    }

    async function loadData(){
      // 進捗バー表示開始
      showProgress(0, '初期化中...');

      // GAS 環境では google.script.run から取得。ローカルでは埋め込みデータを採用。
      const bootstrap = ()=> {
        showProgress(50, 'ローカルデータ読み込み中...');
        setTimeout(()=> {
          applyPayload(fallbackPayload);
          hideProgress();
        }, 100);
      };

      try{
        if(typeof google !== 'undefined' && google.script && google.script.run){
          const pref = (selectedRegion && selectedRegion.prefecture) || '';
          const muni = (selectedRegion && selectedRegion.municipality) || '';
          showProgress(25, 'サーバーからデータ取得中...');

          google.script.run
            .withSuccessHandler(function(payload){
              showProgress(50, 'データ処理中...');
              setTimeout(()=> {
                // found: false のエラーケースをチェック
                if(payload && payload.found === false){
                  console.error('❌ GAS エラー:', payload.error);
                  hideProgress();
                  alert('データ読み込みエラー:\n\n' + payload.error);
                  bootstrap(); // フォールバックでダミーデータを表示
                } else if(payload){
                  applyPayload(payload);
                } else {
                  console.warn('GAS からデータが返却されませんでした。フォールバックを使用します。');
                  bootstrap();
                }
              }, 100);
            })
            .withFailureHandler(function(err){
              console.error('getMapCompleteDataComplete 取得に失敗しました。フォールバック起動。', err);
              hideProgress();
              alert('GAS 関数実行エラー:\n\n' + err.message);
              bootstrap();
            })
            .getMapCompleteDataComplete(pref, muni);
        } else {
          bootstrap();
        }
      }catch(e){
        console.warn('google.script.run 不可。埋め込みデータで起動します。', e);
        bootstrap();
      }
    }

    // 起動
    loadData();
  })();
  </script>
</body>
</html>






