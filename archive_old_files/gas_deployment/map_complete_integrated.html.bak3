    // 選択地域の全申請者データを取得
    function getAllApplicantsForCity(city) {
      // city.applicantsがあればそれを使用、なければ空配列を返す
      // ※ normalizePayload()で既に各cityにapplicantsが配置されている前提
      if (city.applicants && Array.isArray(city.applicants)) {
        return city.applicants;
      }

      // MapComplete完全統合版ではapplicantsデータは含まれないため空配列を返す
      // （Phase 12-14統合ダッシュボードではPhase 1のApplicantsシートが不要）
      return [];
